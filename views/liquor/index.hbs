<h2>Welcome to the Liquor Index</h2>
<ul>
{{#each liquor}}
  <li>{{brand}}</li>
  <li>{{description}}</li>
  <li>{{type}}</li>
  <li>{{size}}</li>
  <li>{{vendor.companyName}}</li>

  <a href="show/{{barcode}}">Read more about this item</a>


<form action="delete-item/{{_id}}" method="POST">
    <button>Delete this item</button>
</form>

<a href="edit-item/{{_id}}">Edit This item's Info</a>
  <br>
 <a href="/liquor/new-item">Create a new item</a>
{{/each}}
</ul>

